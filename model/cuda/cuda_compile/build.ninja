cuda_cflags = -ccbin /usr/bin/gcc-11 -DTORCH_EXTENSION_NAME=Grid_cuda_tools -DTORCH_API_INCLUDE_EXTENSION_H -DPYBIND11_COMPILER_TYPE=\"_gcc\" -DPYBIND11_STDLIB=\"_libstdcpp\" -DPYBIND11_BUILD_ABI=\"_cxxabi1011\" -isystem /home/sulutong/miniconda3/envs/nerf-slt/lib/python3.11/site-packages/torch/include -isystem /home/sulutong/miniconda3/envs/nerf-slt/lib/python3.11/site-packages/torch/include/torch/csrc/api/include -isystem /home/sulutong/miniconda3/envs/nerf-slt/lib/python3.11/site-packages/torch/include/TH -isystem /home/sulutong/miniconda3/envs/nerf-slt/lib/python3.11/site-packages/torch/include/THC -isystem /home/sulutong/cuda/include -isystem /home/sulutong/miniconda3/envs/nerf-slt/include/python3.11 -D_GLIBCXX_USE_CXX11_ABI=0 -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr -gencode=arch=compute_80,code=sm_80 -gencode=arch=compute_89,code=sm_89 --compiler-options '-fPIC' -std=c++17 -lineinfo -use_fast_math --expt-relaxed-constexpr -DCUDA_VERSION=12060
cuda_post_cflags = 
