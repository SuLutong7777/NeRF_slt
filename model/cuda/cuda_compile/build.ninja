ldflags = -shared -L/home/sulutong/miniconda3/envs/nerf-slt/lib/python3.11/site-packages/torch/lib -lc10 -lc10_cuda -ltorch_cpu -ltorch_cuda -ltorch -ltorch_python -L/home/sulutong/cuda/lib64 -lcudart

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc  $cuda_cflags -c $in -o $out $cuda_post_cflags



rule link
  command = $cxx $in $ldflags -o $out

build C_link_file.o: compile /home/sulutong/3D-NeRF-test/NeRF_slt_v3.0/model/cuda/C_link_file.cpp
build A_python_port.cuda.o: cuda_compile /home/sulutong/3D-NeRF-test/NeRF_slt_v3.0/model/cuda/A_python_port.cu
build hash_enc_fd.cuda.o: cuda_compile /home/sulutong/3D-NeRF-test/NeRF_slt_v3.0/model/cuda/src/hash_enc_fd.cu
build hash_enc_bd.cuda.o: cuda_compile /home/sulutong/3D-NeRF-test/NeRF_slt_v3.0/model/cuda/src/hash_enc_bd.cu



build Grid_cuda_tools.so: link C_link_file.o A_python_port.cuda.o hash_enc_fd.cuda.o hash_enc_bd.cuda.o

default Grid_cuda_tools.so
